{
   "name": "我就是要離開社團",
   "version": "0.1.1",
   "description": "討人厭的FB購物社團，我現在就是要離開！",
   "background": {
      "page": "background.html"
   },
   "content_security_policy": "script-src 'self'; img-src *; object-src 'self'",
   "icons": {
      "256": "favicon_256.png",
      "64": "favicon_64.png",
      "16": "favicon_16.png"
   },
   "content_scripts": [ {
      "matches": [
         "http://*.facebook.com/*",
         "https://*.facebook.com/*"
      ],
      "js": ["jquery-1.8.0.min.js", "leave.js"],
      "run_at": "document_end"
   } ],
   "manifest_version": 2,
   "minimum_chrome_version": "20",
   "browser_action": {
      "default_icon": "favicon_256.png",
      "default_title": "檢查並且離開購物社團！"
   },
   "permissions": [
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "http://*.facebook.com/*",
      "https://*.facebook.com/*",
      "http://spamgroup.tonyq.org/groups/json",
      "https://spamgroup.tonyq.org/groups/json"
   ],
   "web_accessible_resources": [ "jquery-1.8.0.min.js", "leave.js" ]
}